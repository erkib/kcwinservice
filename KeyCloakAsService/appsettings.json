{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "yyyy-MM-dd HH:mm:sszz "
      }
    },
    "File": {
      "Path": "C:\\Logs\\kcservicewrapper.log",
      "Append": true,
      "MinLevel": "Information", // min level for the file logger
      "FileSizeLimitBytes": 10485760, // 10 MB
      "MaxRollingFiles": 100
    }
  },
  "Process": {
    "Path": "C:\\Program Files\\keycloak-26.1.4\\keycloak-26.1.4\\bin\\kc.bat",
    "Args": [
      "start-dev"
    ]
  }
}
